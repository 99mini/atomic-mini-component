{
  "name": "@99mini/atom",
  "version": "0.1.1",
  "sideEffects": false,
  "main": "./src/index.ts",
  "files": [
    "dist",
    "scripts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/99mini/frontend-libraries",
    "directory": "packages/atom"
  },
  "scripts": {
    "cp:postinstall": "rm -rf scripts && mkdir scripts && cp ../../scripts/postinstall.sh scripts/postinstall.sh && chmod +x scripts/postinstall.sh",
    "prebuild": "npm run cp:postinstall",
    "build": "rm -rf dist && rollup -c rollup.config.js",
    "deploy:only": "npm publish --access public",
    "deploy": "npm run build && npm run deploy:only",
    "postinstall": "./scripts/postinstall.sh",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@99mini/core": "^0.1.1",
    "@99mini/utils": "^0.1.1",
    "classnames": "^2.5.1"
  },
  "devDependencies": {
    "@99mini/rollup-config": "workspace:^0.1.0",
    "@chromatic-com/storybook": "^1.2.19",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@storybook/addon-essentials": "^8.0.0",
    "@storybook/addon-interactions": "^8.0.0",
    "@storybook/addon-links": "^8.0.0",
    "@storybook/addon-onboarding": "^8.0.0",
    "@storybook/addon-webpack5-compiler-swc": "^1.0.2",
    "@storybook/blocks": "^8.0.0",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^8.0.0",
    "@storybook/react-webpack5": "^8.0.0",
    "@storybook/test": "^8.0.0",
    "@types/react": "^18.2.65",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "css-loader": "^6.10.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-storybook": "^0.8.0",
    "postcss": "^8.4.38",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^4.17.2",
    "sass": "^1.72.0",
    "sass-loader": "^14.1.1",
    "storybook": "^8.0.0",
    "style-loader": "^3.3.4",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typescript": "^5.4.2"
  },
  "peerDependencies": {
    "react": "^18"
  }
}
